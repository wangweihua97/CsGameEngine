<UserControl x:Class="CsGameEngine.WpfItem.PropertyItem.PropertyCheckBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CsGameEngine.WpfItem.PropertyItem"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="250">
    <Canvas Background="White">
        <TextBlock x:Name="name" Width="94" Height="28" Canvas.Left="10" Canvas.Top="4" Text="Name" FontSize="15" />
        <CheckBox x:Name="checkBox" Height="23" Canvas.Left="167" Canvas.Top="4" Width="28" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Checked="checkBox_Checked" Unchecked="checkBox_Unchecked"/>
    </Canvas>
</UserControl>
